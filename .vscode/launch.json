{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run MathEye Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/extension",
        "${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/extension/out/**/*.js"
      ],
      "preLaunchTask": "extension: npm compile"
    },
    {
      "name": "Run MathEye Extension (Open TestGoals.lean)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/extension",
        "--new-window",
        "/Users/sjm/coding/projects/lean-plugin/matheye-proof-builder/TestGoals.lean"
      ],
      "outFiles": [
        "${workspaceFolder}/extension/out/**/*.js"
      ],
      "preLaunchTask": "extension: npm compile"
    },
    {
      "name": "Run MathEye Extension (Open example/Basic.lean)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/extension",
        "--new-window",
        "${workspaceFolder}/example/Basic.lean"
      ],
      "outFiles": [
        "${workspaceFolder}/extension/out/**/*.js"
      ],
      "preLaunchTask": "extension: npm compile"
    },
    {
      "name": "Run MathEye Extension (Workspace File)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/extension",
        "--new-window",
        "${workspaceFolder}/matheye-proof-builder.code-workspace"
      ],
      "outFiles": [
        "${workspaceFolder}/extension/out/**/*.js"
      ],
      "preLaunchTask": "extension: npm compile"
    },
    {
      "name": "Run MathEye Extension (Absolute Root)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/extension",
        "--new-window",
        "/Users/sjm/coding/projects/lean-plugin/matheye-proof-builder"
      ],
      "outFiles": [
        "${workspaceFolder}/extension/out/**/*.js"
      ],
      "preLaunchTask": "extension: npm compile"
    }
  ]
}

{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "env": {
        "MATHEYE_TEST_MODE": "1",
        "MATHEYE_USE_REAL_RPC": "1"
      },
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "${workspaceFolder}/.."
      ],
      "preLaunchTask": "preLaunch"
    }
  ]
}
